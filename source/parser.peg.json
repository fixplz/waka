{
  "rules": [
    {
      "name": "Start",
      "def": {
        "format": "{rules: rules, init: init,}",
        "of": {
          "seq": [
            {
              "ref": "sp"
            },
            {
              "opt": {
                "ref": "Embed"
              },
              "bind": "init"
            },
            {
              "many": {
                "ref": "Rule"
              },
              "bind": "rules"
            }
          ]
        }
      }
    },
    {
      "name": "Rule",
      "def": {
        "format": "{name: name, def: def}",
        "of": {
          "seq": [
            {
              "ref": "RuleHead",
              "bind": "name"
            },
            {
              "ref": "eq"
            },
            {
              "anchor": true
            },
            {
              "ref": "Expr",
              "bind": "def"
            },
            {
              "ref": "semi"
            }
          ]
        }
      }
    },
    {
      "name": "RuleHead",
      "def": {
        "format": "n",
        "of": {
          "seq": [
            {
              "ref": "word",
              "bind": "n"
            },
            {
              "ref": "sp"
            }
          ]
        }
      }
    },
    {
      "name": "Expr",
      "def": {
        "format": "unwrap(alt) || {alt: alt}",
        "of": {
          "many": {
            "delim": {
              "ref": "ExprBlock",
              "bind": "expr"
            },
            "tag": "%sep",
            "token": {
              "ref": "slash"
            }
          },
          "bind": "alt"
        }
      }
    },
    {
      "name": "ExprBlock",
      "def": {
        "format": "format ? { format: format, of: body } : body",
        "of": {
          "seq": [
            {
              "opt": {
                "ref": "ExprSeq"
              },
              "bind": "body"
            },
            {
              "opt": {
                "ref": "FormatEmbed"
              },
              "bind": "format"
            }
          ]
        }
      }
    },
    {
      "name": "ExprSeq",
      "def": {
        "format": "unwrap(atoms) || {seq: atoms}",
        "of": {
          "many": {
            "ref": "ExprStep"
          },
          "bind": "atoms"
        }
      }
    },
    {
      "name": "ExprStep",
      "def": {
        "alt": [
          {
            "format": "atom(bind, expr, op)",
            "of": {
              "seq": [
                {
                  "opt": {
                    "format": "n",
                    "of": {
                      "seq": [
                        {
                          "ref": "word",
                          "bind": "n"
                        },
                        {
                          "ref": "col"
                        }
                      ]
                    }
                  },
                  "bind": "bind"
                },
                {
                  "ref": "ExprVal",
                  "bind": "expr"
                },
                {
                  "opt": {
                    "range": [
                      {
                        "oneof": "?*+"
                      }
                    ],
                    "not": false
                  },
                  "bind": "op"
                },
                {
                  "ref": "sp"
                }
              ]
            }
          },
          {
            "format": "{lookahead: expr || true, not: op == '!', embed: embed}",
            "of": {
              "seq": [
                {
                  "range": [
                    {
                      "oneof": "&!"
                    }
                  ],
                  "not": false,
                  "bind": "op"
                },
                {
                  "alt": [
                    {
                      "ref": "ExprVal",
                      "bind": "expr"
                    },
                    {
                      "ref": "Embed",
                      "bind": "embed"
                    }
                  ]
                }
              ]
            }
          },
          {
            "format": "{anchor: true}",
            "of": {
              "seq": [
                {
                  "str": "%anc"
                },
                {
                  "ref": "sp"
                }
              ]
            }
          }
        ]
      }
    },
    {
      "name": "ExprVal",
      "def": {
        "alt": [
          {
            "ref": "Ref"
          },
          {
            "ref": "Range"
          },
          {
            "ref": "Literal"
          },
          {
            "ref": "ExprParens"
          },
          {
            "format": "{special: 'nl'}",
            "of": {
              "seq": [
                {
                  "str": "%nl"
                },
                {
                  "ref": "sp"
                }
              ]
            }
          },
          {
            "format": "{special: 'any'}",
            "of": {
              "str": "%any"
            }
          },
          {
            "format": "{special: { match: embed }}",
            "of": {
              "seq": [
                {
                  "str": "%match"
                },
                {
                  "ref": "EmbedParens",
                  "bind": "embed"
                }
              ]
            }
          },
          {
            "format": "{special: { embed: embed }}",
            "of": {
              "seq": [
                {
                  "str": "%"
                },
                {
                  "ref": "Embed",
                  "bind": "embed"
                }
              ]
            }
          }
        ]
      }
    },
    {
      "name": "ExprParens",
      "def": {
        "format": "tag ?\n    { delim: expr, tag: tag, token: token }\n    : expr",
        "of": {
          "seq": [
            {
              "str": "("
            },
            {
              "ref": "sp"
            },
            {
              "anchor": true
            },
            {
              "ref": "Expr",
              "bind": "expr"
            },
            {
              "opt": {
                "seq": [
                  {
                    "alt": [
                      {
                        "str": "%sep"
                      },
                      {
                        "str": "%delim"
                      }
                    ],
                    "bind": "tag"
                  },
                  {
                    "ref": "sp"
                  },
                  {
                    "ref": "Expr",
                    "bind": "token"
                  }
                ]
              }
            },
            {
              "str": ")"
            },
            {
              "ref": "sp"
            }
          ]
        }
      }
    },
    {
      "name": "Ref",
      "def": {
        "format": "{ref: n}",
        "of": {
          "seq": [
            {
              "ref": "word",
              "bind": "n"
            },
            {
              "ref": "sp"
            }
          ]
        }
      }
    },
    {
      "name": "Literal",
      "def": {
        "format": "{str: s}",
        "of": {
          "seq": [
            {
              "ref": "str",
              "bind": "s"
            },
            {
              "ref": "sp"
            }
          ]
        }
      }
    },
    {
      "name": "Range",
      "def": {
        "format": "{range: rs, not: not == '^'}",
        "of": {
          "seq": [
            {
              "str": "["
            },
            {
              "anchor": true
            },
            {
              "opt": {
                "str": "^"
              },
              "bind": "not"
            },
            {
              "ref": "sp"
            },
            {
              "any": {
                "alt": [
                  {
                    "format": "{from: from, to: to}",
                    "of": {
                      "seq": [
                        {
                          "ref": "rangechar",
                          "bind": "from"
                        },
                        {
                          "str": "-"
                        },
                        {
                          "ref": "rangechar",
                          "bind": "to"
                        },
                        {
                          "ref": "sp"
                        }
                      ]
                    }
                  },
                  {
                    "format": "{oneof: chars.join('')}",
                    "of": {
                      "many": {
                        "delim": {
                          "many": {
                            "ref": "rangechar"
                          }
                        },
                        "tag": "%delim",
                        "token": {
                          "ref": "sp"
                        }
                      },
                      "bind": "chars"
                    }
                  }
                ]
              },
              "bind": "rs"
            },
            {
              "str": "]"
            },
            {
              "ref": "sp"
            }
          ]
        }
      }
    },
    {
      "name": "rangechar",
      "def": {
        "alt": [
          {
            "ref": "strEsc"
          },
          {
            "range": [
              {
                "oneof": "\\[\\]\\ \\^\\n\\r\\u2028\\u2029"
              }
            ],
            "not": true
          }
        ]
      }
    },
    {
      "name": "FormatEmbed",
      "def": {
        "format": "isobj ? '{' + embed + '}' : embed",
        "of": {
          "seq": [
            {
              "opt": {
                "format": "true",
                "of": {
                  "lookahead": {
                    "seq": [
                      {
                        "str": "{"
                      },
                      {
                        "ref": "sp"
                      },
                      {
                        "ref": "word"
                      },
                      {
                        "any": {
                          "str": " "
                        }
                      },
                      {
                        "ref": "col"
                      }
                    ]
                  },
                  "not": false
                }
              },
              "bind": "isobj"
            },
            {
              "ref": "Embed",
              "bind": "embed"
            }
          ]
        }
      }
    },
    {
      "name": "Embed",
      "def": {
        "format": "embed.trim()",
        "of": {
          "seq": [
            {
              "str": "{"
            },
            {
              "ref": "embed",
              "bind": "embed"
            },
            {
              "str": "}"
            },
            {
              "ref": "sp"
            }
          ]
        }
      }
    },
    {
      "name": "EmbedParens",
      "def": {
        "format": "embed.trim()",
        "of": {
          "seq": [
            {
              "str": "("
            },
            {
              "ref": "embed",
              "bind": "embed"
            },
            {
              "str": ")"
            },
            {
              "ref": "sp"
            }
          ]
        }
      }
    },
    {
      "name": "word",
      "def": {
        "seq": [
          {
            "range": [
              {
                "from": "a",
                "to": "z"
              },
              {
                "from": "A",
                "to": "Z"
              },
              {
                "oneof": "_"
              }
            ],
            "not": false
          },
          {
            "any": {
              "range": [
                {
                  "from": "a",
                  "to": "z"
                },
                {
                  "from": "A",
                  "to": "Z"
                },
                {
                  "from": "0",
                  "to": "9"
                },
                {
                  "oneof": "_"
                }
              ],
              "not": false
            }
          }
        ]
      }
    },
    {
      "name": "str",
      "def": {
        "alt": [
          {
            "format": "x",
            "of": {
              "seq": [
                {
                  "range": [
                    {
                      "oneof": "\""
                    }
                  ],
                  "not": false
                },
                {
                  "any": {
                    "alt": [
                      {
                        "ref": "strEsc"
                      },
                      {
                        "range": [
                          {
                            "oneof": "\"\\n\\r\\u2028\\u2029"
                          }
                        ],
                        "not": true
                      }
                    ]
                  },
                  "bind": "x"
                },
                {
                  "range": [
                    {
                      "oneof": "\""
                    }
                  ],
                  "not": false
                }
              ]
            }
          },
          {
            "format": "x",
            "of": {
              "seq": [
                {
                  "range": [
                    {
                      "oneof": "'"
                    }
                  ],
                  "not": false
                },
                {
                  "any": {
                    "alt": [
                      {
                        "ref": "strEsc"
                      },
                      {
                        "range": [
                          {
                            "oneof": "'\\n\\r\\u2028\\u2029"
                          }
                        ],
                        "not": true
                      }
                    ]
                  },
                  "bind": "x"
                },
                {
                  "range": [
                    {
                      "oneof": "'"
                    }
                  ],
                  "not": false
                }
              ]
            }
          }
        ]
      }
    },
    {
      "name": "strEsc",
      "def": {
        "alt": [
          {
            "seq": [
              {
                "str": "\\\\u"
              },
              {
                "ref": "hex"
              },
              {
                "ref": "hex"
              },
              {
                "ref": "hex"
              },
              {
                "ref": "hex"
              }
            ]
          },
          {
            "seq": [
              {
                "str": "\\\\x"
              },
              {
                "ref": "hex"
              },
              {
                "ref": "hex"
              }
            ]
          },
          {
            "seq": [
              {
                "str": "\\\\"
              },
              {
                "special": "any"
              }
            ]
          }
        ]
      }
    },
    {
      "name": "hex",
      "def": {
        "range": [
          {
            "from": "0",
            "to": "9"
          },
          {
            "from": "a",
            "to": "f"
          },
          {
            "from": "A",
            "to": "F"
          }
        ],
        "not": false
      }
    },
    {
      "name": "embed",
      "def": {
        "seq": [
          {
            "any": {
              "range": [
                {
                  "oneof": "{}()\"'"
                }
              ],
              "not": true
            }
          },
          {
            "any": {
              "seq": [
                {
                  "alt": [
                    {
                      "seq": [
                        {
                          "str": "{"
                        },
                        {
                          "ref": "embed"
                        },
                        {
                          "str": "}"
                        }
                      ]
                    },
                    {
                      "seq": [
                        {
                          "str": "("
                        },
                        {
                          "ref": "embed"
                        },
                        {
                          "str": ")"
                        }
                      ]
                    },
                    {
                      "ref": "str"
                    }
                  ]
                },
                {
                  "any": {
                    "range": [
                      {
                        "oneof": "{}()\"'"
                      }
                    ],
                    "not": true
                  }
                }
              ]
            }
          }
        ]
      }
    },
    {
      "name": "eq",
      "def": {
        "seq": [
          {
            "str": "="
          },
          {
            "ref": "sp"
          }
        ]
      }
    },
    {
      "name": "slash",
      "def": {
        "seq": [
          {
            "str": "/"
          },
          {
            "ref": "sp"
          }
        ]
      }
    },
    {
      "name": "semi",
      "def": {
        "seq": [
          {
            "str": ";"
          },
          {
            "ref": "sp"
          }
        ]
      }
    },
    {
      "name": "col",
      "def": {
        "seq": [
          {
            "str": ":"
          },
          {
            "ref": "sp"
          }
        ]
      }
    },
    {
      "name": "sp",
      "def": {
        "any": {
          "alt": [
            {
              "many": {
                "alt": [
                  {
                    "special": "nl"
                  },
                  {
                    "str": " "
                  }
                ]
              }
            },
            {
              "seq": [
                {
                  "str": "//"
                },
                {
                  "any": {
                    "range": [
                      {
                        "oneof": "\\r\\n"
                      }
                    ],
                    "not": true
                  }
                },
                {
                  "special": "nl"
                }
              ]
            },
            {
              "seq": [
                {
                  "str": "/*"
                },
                {
                  "any": {
                    "seq": [
                      {
                        "lookahead": {
                          "str": "*/"
                        },
                        "not": true
                      },
                      {
                        "alt": [
                          {
                            "special": "nl"
                          },
                          {
                            "special": "any"
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "str": "*/"
                }
              ]
            }
          ]
        }
      }
    }
  ],
  "init": "function unwrap(val) {\n    if(val.length == 1) return val[0]\n  }\n  function atom(bind, expr, op) {\n    var el =\n      op == '+' ? { many: expr }\n      : op == '*' ? { any: expr }\n      : op == '?' ? { opt: expr }\n      : expr\n    if(bind) el.bind = bind\n    return el\n  }"
}